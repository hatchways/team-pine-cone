{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-var": "error",
        "indent": [
            "error",
            2
        ]
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "sourceType": "module"
    }
}